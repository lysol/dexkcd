{
  "name": "dexkcd",
  "version": "1.0",
  "description": "Remove XKCD from the internet",
  "browser_action": {
    "default_icon": "icon.png",
  },
  "permissions": [
    "http://dexkcd.brainindustries.com/",
    "http://jashkenas.github.com/coffee-script/extras/coffee-script.js",
    "http://raw.github.com/lysol/dexkcd/master/dexkcd.coffee",
    "http://ajax.googleapis.com/"
  ],
  "background_page": "background.html"
  "content-scripts": [
    {
      "matches": ["*"],
      "js": ["lib/dexkcd.js"]
    }
  ]
}